{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "{{ .Env.OSC7 }}"
        },
        {
          "properties": {
            "root_icon": "âš¡"
          },
          "style": "powerline",
          "template": " \uf0e7 ",
          "foreground": "#ffeb3b",
          "powerline_symbol": "\ue0b0",
          "background": "#bd0505",
          "type": "root"
        },
        {
          "style": "powerline",
          "template": " \ue61f {{ .Full }} ",
          "foreground": "#49eb33",
          "powerline_symbol": "\ue0b0",
          "background": "#100e23",
          "type": "haskell"
        },
        {
          "style": "powerline",
          "template": " \ue70c {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#AAEFFF",
          "type": "dotnet"
        },
        {
          "style": "powerline",
          "template": " R {{ .Full }} ",
          "foreground": "blue",
          "powerline_symbol": "\ue0b0",
          "background": "lightWhite",
          "type": "r"
        },
        {
          "properties": {
            "display_mode": "context"
          },
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#906cff",
          "type": "python"
        },
        {
          "properties": {
            "display_mode": "files",
            "home_icon": "\uf015",
            "style": "agnoster"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\ue0b0",
          "background": "#22a4d3",
          "type": "path"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_push_status": true,
            "fetch_upstream_icon": true,
            "truncate_symbol": "\u2026",
            "untracked_modes": {
              "*": "all"
            }
          },
          "style": "powerline",
          "template": " {{ url .UpstreamIcon .UpstreamURL }}{{ trunc 30 .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#FF479C>\uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#35FF00>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <#009AFF>\ueb4b {{ .StashCount }}{{ end }}</>{{ if gt .WorktreeCount 0 }} \uf1bb {{ .WorktreeCount }}{{ end }}",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#53ca53",
          "type": "git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFAA3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFEAAF{{ end }}",
            "{{ if gt .Ahead 0 }}#00FFFF{{ end }}",
            "{{ if gt .Behind 0 }}#FAA0AE{{ end }}"
          ]
        },
        {
          "style": "powerline",
          "template": " {{ if eq .Env.POSH \"True\"}}\uf0c7{{end}} ",
          "foreground": "transparent",
          "powerline_symbol": "\ue0b0",
          "background": "#a8cdc9",
          "type": "text"
        }
      ]
    }
  ],
  "version": 3,
  "auto_upgrade": true,
  "shell_integration": true,
  "patch_pwsh_bleed": true,
  "final_space": true
}
